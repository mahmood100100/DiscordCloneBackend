{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=DiscordCloneNewDb;trusted_connection=true;TrustServerCertificate=True;Command Timeout=180"
  },
  "EmailSettings": {
    "SenderEmail": "env:EmailSettings__SenderEmail",
    "Password": "env:EmailSettings__Password",
    "SmtpServer": "smtp.gmail.com",
    "Port": 587,
    "SslUse": false
  },
  "ApiSettings": {
    "SecretKey": "env:ApiSettings__SecretKey"
  },
  "CorsSettings": {
    "DiscordCloneApp": "env:CorsSettings__FrontendUrl"
  },

  "AllowedHosts": "*"
}
