{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "env:POSTGRES_CONNECTION_STRING"
  },
  "EmailSettings": {
    "SenderEmail": "env:EmailSettings__SenderEmail",
    "Password": "env:EmailSettings__Password",
    "SmtpServer": "env:EmailSettings__SmtpServer",
    "Port": "env:EmailSettings__Port",
    "SslUse": false
  },
  "ApiSettings": {
    "SecretKey": "env:ApiSettings__SecretKey"
  },
  "CorsSettings": {
    "DiscordCloneApp": "env:CorsSettings__FrontendUrl"
  },
  "AllowedHosts": "*"
}